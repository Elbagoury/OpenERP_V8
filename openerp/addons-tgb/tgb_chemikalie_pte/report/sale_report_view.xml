<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
<!-- Quotation -->
	    <record id="quotation_report" model="ir.actions.report.xml">
	        <field name="name">quotation_report</field>
	        <field name="type">ir.actions.report.xml</field>
	        <field name="model">sale.order</field>
	        <field name="report_name">quotation_report</field>
	        <field name="report_type">aeroo</field>
	        <field name="in_format">oo-odt</field>
	        <field name="out_format" model="report.mimetypes" search="[('code','=','oo-doc'),('compatible_types','=','oo-odt')]"/>
	        <field name="parser_loc">tgb_chemikalie_pte/report/sale_report.py</field>
	        <field name="report_rml">tgb_chemikalie_pte/report/quotation_report.odt</field>
	        <field name="parser_state">loc</field>
	        <field name="tml_source">file</field>
	        <field name="attachment_use" eval="0"/>
    	</record>		    
		<report 
			auto="False" 
			menu="True"
			id="quotation_report"
			model="sale.order"
			name="quotation_report"
	        string="Quotation"
	        rml="tgb_chemikalie_pte/report/quotation_report.odt"/>
        
<!-- Sales Order -->
	    <record id="sales_order_report" model="ir.actions.report.xml">
	        <field name="name">sales_order_report</field>
	        <field name="type">ir.actions.report.xml</field>
	        <field name="model">sale.order</field>
	        <field name="report_name">sales_order_report</field>
	        <field name="report_type">aeroo</field>
	        <field name="in_format">oo-odt</field>
	        <field name="out_format" model="report.mimetypes" search="[('code','=','oo-doc'),('compatible_types','=','oo-odt')]"/>
	        <field name="parser_loc">tgb_chemikalie_pte/report/sale_report.py</field>
	        <field name="report_rml">tgb_chemikalie_pte/report/sales_order_report.ods</field>
	        <field name="parser_state">loc</field>
	        <field name="tml_source">file</field>
	        <field name="attachment_use" eval="0"/>
    	</record>		    
		<report 
			auto="False" 
			menu="True"
			id="sales_order_report"
			model="sale.order"
			name="sales_order_report"
	        string="Sales Order"
	        rml="tgb_chemikalie_pte/report/sales_order_report.odt"/>
        
	</data>
</openerp>
