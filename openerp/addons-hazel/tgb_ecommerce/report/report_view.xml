<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
<!-- Order Form -->
	    <record id="order_form_report" model="ir.actions.report.xml">
	        <field name="name">order_form_report</field>
	        <field name="type">ir.actions.report.xml</field>
	        <field name="model">crm.phonecall</field>
	        <field name="report_name">order_form_report</field>
	        <field name="report_type">aeroo</field>
	        <field name="in_format">oo-odt</field>
	        <field name="out_format" model="report.mimetypes" search="[('code','=','oo-pdf'),('compatible_types','=','oo-odt')]"/>
	        <field name="parser_loc">tgb_ecommerce/report/order_form_report.py</field>
	        <field name="report_rml">tgb_ecommerce/report/order_form_report.odt</field>
	        <field name="parser_state">loc</field>
	        <field name="tml_source">file</field>
	        <field name="attachment_use" eval="0"/>
    	</record>		    
		<report 
			auto="False" 
			menu="False"
			id="order_form_report"
			model="crm.phonecall"
			name="order_form_report"
	        string="Order Form"
	        rml="tgb_ecommerce/report/order_form_report.odt"/>
	        
