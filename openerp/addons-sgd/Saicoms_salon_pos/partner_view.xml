<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
		
		<record id="saicoms_view_partner_form" model="ir.ui.view">
            <field name="name">saicoms.res.partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='title']" position="after">
            		<field name="customer_type_id" attrs="{'invisible':[('customer','!=',True)]}"/>
                    <field name="customer_source_id" attrs="{'invisible':[('customer','!=',True)]}"/>/>
            	</xpath>
                <xpath expr="//field[@name='website']" position="after">
            		<field name="saicoms_dob" attrs="{'invisible':[('customer','!=',True)]}"/>
                    <field name="saicoms_occupation" attrs="{'invisible':[('customer','!=',True)]}"/>/>
            	</xpath>
            </field>
		</record>

    </data>
</openerp>