<openerp>
    <data>

        <record id="dksquare_contract_import_view" model="ir.ui.view">
            <field name="name">Construction Employee Import</field>
            <field name="model">dksquare.contract.import</field>
            <field name="arch" type="xml">
                <form string="Import Employee" version="7.0">
                    <sheet>
                        <group>
                            <field name="file"/>
                            <button name="import_employee" type="object" string="Import Employee"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="dksquare_contract_import_action">
            <field name="name">Construction Employee Import</field>
            <field name="view_id" ref="dksquare_contract_import_view"/>
            <field name="res_model">dksquare.contract.import</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="usage">menu</field>
            <field name="target">new</field>
        </record>

        <menuitem id="dksquare_contract_import_menu" action="dksquare_contract_import_action" name="Import Employees"
                  parent="menu_construction_configuration" sequence="10"/>

    </data>
</openerp>
